/*
FileName : T5_29.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
double a,b;
a=24;
b=0.05;

for( int i=1;i<=6;i++){
cout<<"When the interest rate is :"<<b<<endl;
    for(int j=1;j<=387;j++){
        a=a*(1+b);

    }

cout<<a<<endl;
    b=b+0.01;
    a=24;
}







return 0;
}

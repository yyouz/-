/*
FileName : T5_12.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
    int b=1;
for(int n=1;n<=10;n++){
    for(int i=1;i<=b;i++)
{cout<<"*";
}
b++;
cout<<endl;
}
cout<<endl;
int a=1;
for(int c=1;c<=10;c++){
    for(int d=10;d>=a;d--){
        cout<<"*";
    }
    a++;
    cout<<endl;
}cout<<endl;
int h=1;
int j=1;
for(int e=1;e<=10;e++){
    for(int f=10;f>=j;f--){
        cout<<"*";
    }
    j++;
    cout<<endl;
for(int g=1;g<=h;g++){
    cout<<" ";
}
h++;
}
cout<<endl;
int m=9;
int o=1;
for(int k=1;k<=10;k++){
 for(int l=1;l<=m;l++){
    cout<<" ";
 }
for(int p=1;p<=o;p++){
    cout<<"*";
}
cout<<endl;
    o++;
    m--;


}




return 0;
}


#include<iostream>
using namespace std;
int main(){
cout<<"the fist of pi"<<endl;
cout<<"pi = 4"<<endl;
cout<<"the two project of pi"<<endl;
cout<<"pi = 2.6666666666666666........"<<endl;
cout<<"the thousand projects of pi"<<endl;
double s;
double a=1;
s=4;
for(int i=1;i<=499;i++){
a++;
s=s-(4/a);
 a=a+2;
s=s+(4/a);

}
s=s-(4/a);
cout<<"pi ="<<s<<endl;




return 0;
}

/*
FileName : T5_18.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
int side1,side2,hypotenuse;
for(side1=1;side1<=500;side1++){
    for(side2=side1;side2<=500;side2++){
      for(hypotenuse=side2;hypotenuse<=500;hypotenuse++){
        if(side1*side1+side2*side2==hypotenuse*hypotenuse){
            cout<<"{"<<side1<<","<<side2<<","<<hypotenuse<<"}"<<" ";

        }
      }

    }
}
cout<<"these are all the situation!"<<endl;


return 0;
}

/*
FileName : T5_21.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
    cout<<"Please input the number to work out the salary"<<endl;
int n;
    cin>>n;
switch(n){
case 1 :
    cout<<"the manager's salary is x"<<endl;
    break;
case 2 :
cout<<"the work hours are n"<<endl;
        cout<<"if the n<=40,the salary is xn"<<endl<<"if the n>40,the salary is the x*n+(n-40)*1.5*x"<<endl;
break;
case 3 :
    cout<<"the sales money is h"<<endl;
cout<<"the salary is 250+h*0.057"<<endl;
break;
case 4 :
cout<<"if the sales is n,the single piece amount is x"<<endl;
cout<<"the salary is n*x"<<endl;
break;
default :
    cout<<"ERROR!"<<endl;
    break;
}


return 0;
}

/*
FileName : T5_22.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
    int x,y;
    cout<<"!(x<5)&&!(y>=7)==!((x<5)||(y>=7))"<<endl<<"!(a==b)||!(g!=5)==!((a==b)&&(g!=5))"<<endl;
        cout<<"!((x<=8)&&(y>4))==!(x<=8)||!(y>4)"<<endl<<"!((i>4)||(j<=6))==!(i>4)&&!(j<=6)"<<endl;

cout<<"the examples under the line"<<endl<<"please input the x,y"<<endl;
cin>>x>>y;
if(!(x<5)&&!(y>=7)){
    cout<<"a";
}else{cout<<"b";}
cout<<endl;
cout<<"the equal project"<<endl;
if(!((x<5)||(y>=7))){
        cout<<"a";}else{cout<<"b";}
cout<<"the next project"<<endl;
cout<<"input a ,b,g"<<endl;
int a,b,g;
cin>>a>>b>>g;
if(!(a==b)||!(g!=5)){cout<<"a";}else{cout<<"g";}
cout<<endl<<"the equal project"<<endl;
if(!((a==b)&&(g!=5))){cout<<"a";}else{cout<<"g";}
cout<<endl<<"the next project"<<endl;
if(!((x<=8)&&(y>4))){cout<<"a";}else{cout<<"b";}
cout<<endl<<"the equal project"<<endl;
if(!(x<=8)||!(y>4)){cout<<"a";}else{cout<<"b";}
cout<<endl<<"the next project ;please input the i,j"<<endl;
int i,j;
cin>>i>>j;
if(!((i>4)||(j<=6))){cout<<"a";}else{cout<<"b";}
cout<<endl<<"the equal project"<<endl;
if(!(i>4)&&!(j<=6)){cout<<"a";}else{cout<<"b";}
return 0;
}

/*
FileName : T5_23.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
    int a=1;
    int b=1;
for(int i=1;i<=5;i++){
for(int x=4;x>=a;x--){
    cout<<" ";
}
for(int j=1;j<=b;j++){
    cout<<"*";
}
b+=2;
a++;
cout<<endl;
}
int c=1;
int d=1;
for(int h=1;h<=4;h++){
 for(int s=1;s<=c;s++){
    cout<<" ";
 }
 for(int t=7;t>=d;t--){
    cout<<"*";
 }
  c++;
  d=d+2;
  cout<<endl;
}
return 0;
}

/*
FileName : T5_24.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
	int n;
	cout<<"Enter n that is odd number pls"<<endl;
	cin>>n;
	for(int i=1;i<=(n+1)/2;i++)
	{
		for(int j=1;j<=(n+1)/2-i;j++)
		{
			cout<<" ";
		}
		for(int k=1;k<=2*i-1;k++)
		{
			cout<<"*";
		}
		cout<<endl;
	}
	for(int i=1;i<=(n-1)/2;i++)
	{
		for(int j=1;j<=i;j++)
		{
			cout<<" ";
		}
		for(int k=n-2*i;k>=1;k--)
		{
			cout<<"*";
		}
		cout<<endl;
	}










return 0;
}


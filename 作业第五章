College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
char n;
cout<<"Please input your grades(if you want to leave ,please input the G)"<<endl;
int a,b,c,d,e,f;
a=b=c=d=e=f=0;
for(int i=1;i<=99999;i++){
        cin>>n;
if(n==71){
    break;
}
switch(n){
 case 65 :
     a++;
break;
 case 66 :
    b++;
    break;
 case 67 :
    c++;
    break;
 case 68 :
    d++;
    break;
 case 69 :
    e++;
break;
 default :
    f++;
break;
}}
cout<<"THE average grades under the line"<<endl;
cout<<"A1 :"<<a*4<<endl<<"B1 :"<<b*3<<endl<<"C1 :"<<c*2<<endl<<"D1 :"<<d*1<<endl<<"E1 : 0"<<endl<<"F1  : 0";


return 0;
}


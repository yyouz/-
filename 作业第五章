College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
int n,x,min,b;
cout<<"how many numbers do you want to input?"<<endl;
cin>>n;
int arr[n];
b=0;
for(int a=1;a<=n;a++){
  cin>>arr[b];
    b++;

}
min=arr[0];
for(int c=0;c<n;c++){
    if(min>arr[c+1]){
        min=arr[c+1];

    }


}


cout<<min;

return 0;
}


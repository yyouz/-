/*
FileName : T2_21.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just for the homework

*/
#include<iostream>
using namespace std;
int main(){
cout<<"*********"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*       *"<<"\n"<<"*********"<<"\n";

cout<<"  ***  "<<"\n"<<" *     *"<<"\n"<<endl;
for(int i=1;i<=5;i++){
    cout<<"*       *"<<"\n";
    }
cout<<"  ***  "<<"\n"<<endl;
cout<<"  *  "<<"\n"<<" *** "<<"\n"<<"*****"<<"\n";
for(int x =1;x<=6;x++){
    cout<<"  *  "<<"\n";
}
cout<<"    *    "<<"\n";
cout<<"   * *   "<<"\n";
cout<<"  *   *  "<<"\n";
cout<<" *     * "<<"\n";
cout<<"*       *"<<"\n";
cout<<" *     * "<<"\n";
cout<<"  *   *  "<<"\n";
cout<<"   * *   "<<"\n";
cout<<"    *    "<<"\n";

return 0;
}


/*
FileName : T2_19.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just for the homework

*/
#include<iostream>
using namespace std;
int main(){
int a,b,c;
cin>>a>>b>>c;
cout<<"Input three different integers :"<<a<<" "<<b<<" "<<c<<endl;
int sum,ave,pro,min,max;
sum= a+b+c;
ave= sum/3;
pro= a*b*c;
if(a<b){
    min=a;
    max=b;
}else{min=b;max=a;    }
if(max<c){max=c;}
if(min>c){c=min;}
cout<<"Sum is"<<" "<<sum<<"\n"<<"Average is "<<ave<<"\n";
cout<<"Product is "<<pro<<"\n"<<"Smallest is "<<min<<"\n";
cout<<"Largest is "<<max;















return 0;
}


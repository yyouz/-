#include<iostream>
using namespace std;
int main(){
cout<<"********"<<"\n";
cout<<" ********"<<"\n";
cout<<"********"<<"\n";
cout<<" ********"<<"\n";
cout<<"********"<<"\n";
cout<<" ********"<<"\n";
cout<<"********"<<"\n";
cout<<" ********"<<"\n";
cout<<"        "<<endl;
//the second method
for(int i=1;i<=4;i++){
    cout<<"********"<<"\n";
cout<<" ********"<<"\n";
}









return 0;
}


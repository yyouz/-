/*
FileName : T2_23.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
int a,b,c,d,e,min,max;
cin>>a>>b>>c>>d>>e;
if(a<b){min=a;max=b;}else{min=b;max=a;}
if(max<c){max=c;}
if(max<d){max=d;}
if(max<e){max=e;}
if(min>c){min=c;}
if(min>d){min=d;}
if(min>e){min=e;}
cout<<"���ֵ"<<max<<"\n";
cout<<"��Сֵ"<<min;








return 0;
}


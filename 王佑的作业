    long long int people_i0,people_up0;
    uprate=1.009;
    people_i=76;
    cout<<"���\t\t"<<"Ԥ���˿�\t"<<"Ԥ���˿�������\t"<<endl;
    for(int i=1;i<=75;i++)
    {people_pro=people_i;
        people_i=people_i*uprate;
        people_up=(people_i-people_pro);
        people_i0=people_i*100000000;
        people_up0=people_up*100000000;
cout<<"��"<<i<<"��:  \t"<<people_i0<<"\t"<<people_up0<<endl;

    }

cin>>a>>b>>c>>d>>e;
f=b+c-d;
cout<<"�˺�   :"<<a<<"\n"<<"�³�Ƿ��     :"<<b<<"\n"<<"������   :"<<c<<"\n"<<"������      :"<<d<<"\n"<<"���ö��      :"<<e<<"\n"<<"�µ�Ƿ��   :"<<f<<"\n";
  if(f>e){

    cout<<"Credit Limit Exceed"<<endl;
}



}








return 0;
}


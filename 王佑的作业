/*
FileName : T4_19.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main()
{
	int arr[10],m;
	for(int i=0;i<=9;i++)
	{
		cin>>arr[i];
	}
	for(int j=0;j<=9;j++)
	{
		if(arr[j]>arr[0])
		{
			m=arr[j];
			arr[j]=arr[0];
			arr[0]=m;
		}
	}
	cout<<arr[0]<<"  ";
	m=arr[1];
	for(int n=1;n<=9;n++)
	{
		if(arr[n]>arr[1])
		{
			m=arr[n];
			arr[n]=arr[1];
			arr[1]=m;
		}
	}
	cout<<arr[1];
	return 0;
 }








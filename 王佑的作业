/*
FileName : T4_17.cpp
Author   : Wangyou
E-mail   : 2590666971@qq.com
Date     : Mar 6th,2022
College  : School of Computer Science and Information Engineering
Function : Just foe the homework

*/
#include<iostream>
using namespace std;
int main(){
int cou,num,max;
int arr[9];
cou=0;
 cin>>num;
 arr[cou]=num;
 max=num;
while(cou<9){
cout<<"Counter  :" <<cou<<"\n"<<"number     :"<<num<<"\n"<<"Largest   :"<<max;
  cin>>num;
    cou++;
arr[cou]=num;

if(arr[cou]>max){

    max=arr[cou];

}



}





return 0;
}

